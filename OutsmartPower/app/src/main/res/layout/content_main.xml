<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.outsmart.uitestapplication.MainActivity"
    tools:showIn="@layout/app_bar_main"
    android:background="@color/backgroundColorContent"><![CDATA[>


    ]]>

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/wifiListFragmentContainer"></FrameLayout>

    <TextView
        android:id="@+id/deviceNameTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Outsmart Device"
        android:textAlignment="center"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/deviceNameTV"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="183dp"
        android:layout_marginEnd="183dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="210dp"
        android:id="@+id/outlet1Layout">

        <ToggleButton
            android:id="@+id/turnOnOFF1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="@dimen/powerLayoutMargin"
            android:text="ToggleButton" />

        <TextView
            android:id="@+id/powerTV1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:text="Current Power"
            android:textAlignment="center" />

        <Button
            android:id="@+id/stat1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="@dimen/powerLayoutMargin"
            android:text="Statistics" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/outlet2Layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/deviceNameTV"
        android:layout_marginLeft="183dp"
        android:layout_marginStart="183dp"
        android:layout_toEndOf="@+id/wifiListFragmentContainer"
        android:layout_toRightOf="@+id/wifiListFragmentContainer"
        android:layout_marginBottom="210dp"
        android:layout_marginTop="15dp">

        <ToggleButton
            android:id="@+id/turnOnOFF2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="@dimen/powerLayoutMargin"
            android:text="ToggleButton" />

        <TextView
            android:id="@+id/powerTV2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:text="Current Power"
            android:textAlignment="center" />

        <Button
            android:id="@+id/stat2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="@dimen/powerLayoutMargin"
            android:text="Statistics" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/deviceNameTV"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="183dp"
        android:layout_marginEnd="183dp"
        android:layout_marginTop="210dp"
        android:id="@+id/outlet3Layout">

        <ToggleButton
            android:id="@+id/turnOnOFF3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="@dimen/powerLayoutMargin"
            android:text="ToggleButton" />

        <TextView
            android:id="@+id/powerTV3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:text="Current Power"
            android:textAlignment="center" />

        <Button
            android:id="@+id/stat3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="@dimen/powerLayoutMargin"
            android:text="Statistics" />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/outlet4Layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/deviceNameTV"
        android:layout_marginLeft="183dp"
        android:layout_marginStart="183dp"
        android:layout_toEndOf="@+id/wifiListFragmentContainer"
        android:layout_toRightOf="@+id/wifiListFragmentContainer"
        android:layout_marginTop="210dp">

        <ToggleButton
            android:id="@+id/turnOnOFF4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="@dimen/powerLayoutMargin"
            android:text="ToggleButton" />

        <TextView
            android:id="@+id/powerTV4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:text="Current Power"
            android:textAlignment="center" />

        <Button
            android:id="@+id/stat4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="@dimen/powerLayoutMargin"
            android:text="Statistics" />

    </RelativeLayout>


</RelativeLayout>
